{
	"name": "@mailchain/internal",
	"version": "0.18.4",
	"description": "Mailchain internal library",
	"license": "Apache-2.0",
	"author": {
		"name": "Mailchain",
		"email": "support@mailchain.co",
		"url": "https://mailchain.com/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mailchain/mailchain-sdk-js.git",
		"directory": "internal"
	},
	"bugs": {
		"url": "https://github.com/mailchain/mailchain-sdk-js/issues"
	},
	"dependencies": {
		"@mailchain/addressing": "0.18.4",
		"@mailchain/api": "0.18.4",
		"@mailchain/crypto": "0.18.4",
		"@mailchain/encoding": "0.18.4",
		"@mailchain/keyring": "0.18.4",
		"@mailchain/message-composer": "0.18.4",
		"@mailchain/signatures": "0.18.4",
		"@noble/hashes": "^1.3.0",
		"axios": "1.3.4",
		"canonicalize": "^1.0.8",
		"email-addresses": "5.0.0",
		"emailjs-mime-parser": "^2.0.7",
		"lodash": "^4.17.21",
		"long": "^5.2.1",
		"protobufjs": "7.1.1"
	},
	"devDependencies": {
		"jest-mock-extended": "^3.0.1"
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production rollup -c --bundleConfigAsCjs",
		"test": "run -T jest",
		"lint:test": "yarn run -T eslint --ext js,ts,tsx src",
		"lint:fix": "yarn run -T eslint --fix --ext js,ts,tsx src"
	},
	"typesVersions": {
		"*": {
			"identityKeys": [
				"./types/identityKeys/index.d.ts"
			],
			"messagingKeys": [
				"./types/messagingKeys/index.d.ts"
			],
			"sending": [
				"./types/sending/index.d.ts"
			],
			"sending/mail": [
				"./types/sending/mail/index.d.ts"
			]
		}
	},
	"exports": {
		".": {
			"import": "./browser/index.js",
			"require": "./node/index.js",
			"types": "./types/index.d.ts"
		},
		"./identityKeys": {
			"types": "./types/identityKeys/index.d.ts",
			"import": "./browser/identityKeys/index.js",
			"require": "./node/identityKeys/index.js"
		},
		"./messagingKeys": {
			"types": "./types/messagingKeys/index.d.ts",
			"import": "./browser/messagingKeys/index.js",
			"require": "./node/messagingKeys/index.js"
		},
		"./sending": {
			"types": "./types/sending/index.d.ts",
			"import": "./browser/sending/index.js",
			"require": "./node/sending/index.js"
		},
		"./sending/mail": {
			"types": "./types/sending/mail/index.d.ts",
			"import": "./browser/sending/mail/index.js",
			"require": "./node/sending/mail/index.js"
		},
		"./node/configuration": {
			"require": "./node/configuration.js"
		},
		"./node/mailchainResult": {
			"require": "./node/mailchainResult.js"
		},
		"./node/messagingKeys": {
			"require": "./node/messagingKeys/index.js"
		},
		"./node/identityKeys": {
			"require": "./node/identityKeys/index.js"
		},
		"./node/mailbox": {
			"require": "./node/mailbox/index.js"
		},
		"./node/sending": {
			"require": "./node/sending/index.js"
		},
		"./node/sending/mail": {
			"require": "./node/sending/mail/index.js"
		},
		"./node/sending/mail/convertSendMailParams": {
			"require": "./node/sending/mail/convertSendMailParams.js"
		},
		"./node/user": {
			"require": "./node/user/index.js"
		}
	}
}
